# Создать (программно) текстовый файл, записать в него программно набор чисел, разделенных пробелами.
# Программа должна подсчитывать сумму чисел в файле и выводить ее на экран.

с  open ( 'task05.txt' , 'w' ) as  my_files :   # менеджер контекста - открываем на запись
    мои_файлы . write ( "1 2 3 4 5 6 7 8 9" )   # запись строки в файле

с  open ( 'task05.txt' ) as  my_files :   # менеджер контекста - открываем на чтение
    sum_num  =  0   # переменная, в которую будет записана сумма
    для  el  в  my_files :   # цикл, перебор строк в файле
        num_list  =  эл . split ()   # разбиваем строку на цифры
        для  x  в  num_list :   # цикл, перебираем цифры
            sum_num  + =  int ( x )   # суммируем цифры
    print ( f "Числа из файла: { num_list } " )   # вывод
    print ( f "Сумма чисел равна { sum_num } " )   # вывод
